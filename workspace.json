{
  "version": 1,
  "projects": {
    "wishlist": {
      "root": "apps/wishlist",
      "sourceRoot": "apps/wishlist",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/wishlist",
            "outputPath": "dist/apps/wishlist"
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "wishlist:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "wishlist:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/wishlist/.eslintrc",
            "tsConfig": [
              "apps/wishlist/tsconfig.json",
              "apps/wishlist/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/wishlist/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/wishlist/jest.config.js",
            "tsConfig": "apps/wishlist/tsconfig.spec.json"
          }
        }
      }
    },
    "wishlist-e2e": {
      "root": "apps/wishlist-e2e",
      "sourceRoot": "apps/wishlist-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wishlist-e2e/cypress.json",
            "tsConfig": "apps/wishlist-e2e/tsconfig.e2e.json",
            "devServerTarget": "wishlist:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wishlist:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/wishlist-e2e/.eslintrc",
            "tsConfig": ["apps/wishlist-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/wishlist-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/api/.eslintrc",
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "product": {
      "root": "apps/product",
      "sourceRoot": "apps/product",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/product",
            "outputPath": "dist/apps/product"
          }
        },
        "serve": {
          "builder": "@nrwl/next:dev-server",
          "options": {
            "buildTarget": "product:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "product:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/product/.eslintrc",
            "tsConfig": [
              "apps/product/tsconfig.json",
              "apps/product/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/product/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/product/jest.config.js",
            "tsConfig": "apps/product/tsconfig.spec.json"
          }
        }
      }
    },
    "product-e2e": {
      "root": "apps/product-e2e",
      "sourceRoot": "apps/product-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/product-e2e/cypress.json",
            "tsConfig": "apps/product-e2e/tsconfig.e2e.json",
            "devServerTarget": "product:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "product:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/product-e2e/.eslintrc",
            "tsConfig": ["apps/product-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/product-e2e/**"]
          }
        }
      }
    },
    "data": {
      "root": "libs/data",
      "sourceRoot": "libs/data/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/data/.eslintrc",
            "tsConfig": [
              "libs/data/tsconfig.lib.json",
              "libs/data/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/data/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/data/jest.config.js",
            "tsConfig": "libs/data/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "wishlist"
}
